#class {RL_FUNCTION} {kill};
#class {RL_FUNCTION} {open};

#FUNCTION {member_of} {
    #NOP TODO: Use #LIST index?;
    #FOREACH {%1} {item} {
        #IF {"$item" == "%2"} {
            #RETURN {1};
        };
    };
    #RETURN {0};
};

/*
 * Function @deltaColor{number}
 */
#FUNCTION {deltaColor}
{
    #IF {%1 > 0} {
        #RETURN {<128>};
    };
    #ELSEIF {%1 < 0} {
        #RETURN {<118>};
    };
    #ELSE {
        #RETURN {<138>};
    }
}

#class {RL_FUNCTION} {close};
